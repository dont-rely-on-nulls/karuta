multyply[N, 0, 0] :- int[N].
multyply[0, N, 0] :- debug, int[N].
multyply[X, Y, Out] :- int[X], int[Y], multyply[X, Y, 0, Out].

multyply[1, Y, Acc, Out] :- plus[Acc, Y, Out].
multyply[N, Y, Acc, Out] :-
    plus[Acc, Y, NewAcc],
    plus[N, -1, Prev],
    multyply[Prev, Y, NewAcc, Out].

multyply[4, 3, Out]?

